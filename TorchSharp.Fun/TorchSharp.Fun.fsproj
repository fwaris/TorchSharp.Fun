<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Authors>Faisal Waris</Authors>
    <Description>Very rough functional wrapper over torchsharp. Consider this as an alpha version.</Description>
    <Copyright>Copyright © 2021</Copyright>
    <PackageProjectUrl>https://github.com/fwaris/TorchSharpFun</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fwaris/TorchSharpFun</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>0.4.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Test.fsx" />
    <Compile Include="TorchSharp.Fun.fs" />
  </ItemGroup>

  <ItemGroup />

  <ItemGroup>
    <PackageReference Include="FsPickler" Version="5.3.2" />
    <PackageReference Include="TorchSharp" Version="0.96.3" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="6.0.1" />
  </ItemGroup>

</Project>
